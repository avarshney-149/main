<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <actionCalls>
        <name>Execute_Git_Snapshot</name>
        <label>Execute Git Snapshot</label>
        <locationX>398</locationX>
        <locationY>428</locationY>
        <actionName>CopadoSnapshot.executeGitSnapshot</actionName>
        <actionType>externalService</actionType>
        <connector>
            <targetReference>Update_Deployment_Job_with_Result</targetReference>
        </connector>
        <flowTransactionModel>CurrentTransaction</flowTransactionModel>
        <inputParameters>
            <name>api_key</name>
            <value>
                <elementReference>copadoApiKey</elementReference>
            </value>
        </inputParameters>
        <inputParameters>
            <name>gitSnapshotId</name>
            <value>
                <elementReference>copadoSnapshotId</elementReference>
            </value>
        </inputParameters>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </actionCalls>
    <actionCalls>
        <name>Get_Snapshot_Status</name>
        <label>Get Snapshot Status</label>
        <locationX>1146</locationX>
        <locationY>428</locationY>
        <actionName>CopadoAPI.getJobStatus</actionName>
        <actionType>externalService</actionType>
        <connector>
            <targetReference>Did_job_complete</targetReference>
        </connector>
        <flowTransactionModel>CurrentTransaction</flowTransactionModel>
        <inputParameters>
            <name>api_key</name>
            <value>
                <elementReference>copadoApiKey</elementReference>
            </value>
        </inputParameters>
        <inputParameters>
            <name>copadoJobId</name>
            <value>
                <elementReference>Execute_Git_Snapshot.200.copadoJobId</elementReference>
            </value>
        </inputParameters>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </actionCalls>
    <apiVersion>50.0</apiVersion>
    <decisions>
        <name>Did_job_complete</name>
        <label>Did job complete?</label>
        <locationX>1138</locationX>
        <locationY>654</locationY>
        <defaultConnector>
            <targetReference>Wait_for_Snapshot_Completion</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Job still in progress</defaultConnectorLabel>
        <rules>
            <name>Job_succeeded</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Snapshot_Status.200.isFinished</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Snapshot_Status.200.isSuccess</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Again</targetReference>
            </connector>
            <label>Job succeeded</label>
        </rules>
        <rules>
            <name>Job_failed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Snapshot_Status.defaultExc.error</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Again</targetReference>
            </connector>
            <label>Job failed</label>
        </rules>
    </decisions>
    <decisions>
        <name>Did_Snapshot_Start</name>
        <label>Did Snapshot Start?</label>
        <locationX>750</locationX>
        <locationY>430</locationY>
        <defaultConnector>
            <targetReference>Update_Again</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Snapshot Not Started</defaultConnectorLabel>
        <rules>
            <name>Snapshot_Started</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Execute_Git_Snapshot.200.copadoJobId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Wait_for_Snapshot_Completion</targetReference>
            </connector>
            <label>Snapshot Started</label>
        </rules>
    </decisions>
    <description>Execute a Copado Git Snapshot and update the results.</description>
    <formulas>
        <name>CurrentTimePlus3Minutes</name>
        <dataType>DateTime</dataType>
        <expression>{!$Flow.CurrentDateTime}+(3/1440)</expression>
    </formulas>
    <formulas>
        <name>JobStatus</name>
        <dataType>String</dataType>
        <expression>IF({!Job_succeeded}, &apos;Success&apos;, &apos;Failed&apos;)</expression>
    </formulas>
    <formulas>
        <name>Message</name>
        <dataType>String</dataType>
        <expression>IF( ISBLANK({!Execute_Git_Snapshot.defaultExc.error}) , {!Get_Snapshot_Status.defaultExc.error}, {!Execute_Git_Snapshot.defaultExc.error})</expression>
    </formulas>
    <formulas>
        <name>SalesforceOrgURL</name>
        <dataType>String</dataType>
        <expression>LEFT({!$Api.Partner_Server_URL_340}, FIND( &apos;/services&apos;, {!$Api.Partner_Server_URL_340}))</expression>
    </formulas>
    <interviewLabel>Execute Git Snapshot {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Execute Git Snapshot</label>
